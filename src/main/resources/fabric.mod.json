{
	"schemaVersion": 1,
	"id": "hexlink",
	"version": "${version}",
	"name": "Hexlink",
	"description": "Hex casting cool addon.",
	"authors": [
		"jempasam"
	],
	"contact": {
		"sources": "https://github.com/Jempasam/Hexlink"
	},
	"contributors": [
		"jempasam",
		"ChuijkYahus"
	],
	"license": "MIT",
	"icon": "assets/hexlink/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"value": "jempasam.hexlink.HexlinkMod",
				"adapter": "kotlin"
			}
		],
		"client": [
			{
				"value": "jempasam.hexlink.HexlinkModClient",
				"adapter": "kotlin"
			}
		],
		"cardinal-components": [
			"jempasam.hexlink.cc.HexlinkComponents"
		],
		"emi": [
			"jempasam.hexlink.emi.HexlinkEMIPlugin"
		]
	},
	"mixins": [
		"hexlink.mixins.json",
		{
			"config": "hexlink.client.mixins.json",
			"environment": "client"
		}
	],

	"custom": {
		"cardinal-components": [
			"hexlink:world_data"
		]
	},

	"depends": {
		"fabricloader": ">=0.14.21",
		"minecraft": "~1.19.2",
		"java": ">=17",
		"fabric-api": "*",
		"fabric-language-kotlin": ">=1.9.0"
	},

	"suggests": {
		"another-mod": "*"
	}
}